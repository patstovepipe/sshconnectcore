{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/SSHConnectCore/SSHConnectCore.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build Release",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/SSHConnectCore/SSHConnectCore.csproj",
                "-c",
                "Release"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/src/SSHConnectCore/SSHConnectCore.csproj",
                "-c",
                "Release"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish, delete and rename",
            "command": "./rm-mv_script.sh",
            "type": "shell",
            "dependsOn" : "publish"
        },
        {
            "label": "parallel example",
            "dependsOn": [ "publish", "delete and rename" ]
        }
    ]
}